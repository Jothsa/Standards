{
  "name": "jothsasite",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": " vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:style": "stylelint '**/*.{css,postcss,astro,svelte}'",
    "lint:es": "eslint .",
    "lint:prettier": "prettier --cache --plugin-search-dir . --check .",
    "lint": " pnpm lint:prettier; pnpm lint:es; pnpm lint:style",
    "format:style": "stylelint --fix '**/*.{css,postcss,astro,svelte}'",
    "format:es": "eslint --fix",
    "format:prettier": "prettier --cache --plugin-search-dir . --write .",
    "format": "pnpm format:prettier; pnpm format:es; pnpm format:style",
    "pb-typegen": "source .env; pocketbase-typegen -u $PUBLIC_PB_URL -e $PB_EMAIL -p $PB_PASSWORD --out ./src/lib/pocketbase/pocketbase-types.ts"
  },
  "devDependencies": {
    "@iconify/icons-material-symbols": "^1.2.32",
    "@iconify/icons-mdi": "^1.2.44",
    "@iconify/icons-ph": "^1.2.3",
    "@iconify/svelte": "^3.1.0",
    "@playwright/test": "^1.31.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.13.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fontaine": "^0.3.1",
    "open-props": "^1.5.6",
    "pocketbase-typegen": "^1.1.7",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-jit-props": "^1.0.11",
    "postcss-load-config": "^4.0.1",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.5",
    "prettier-plugin-svelte": "^2.8.1",
    "stylelint": "^15.3.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-high-performance-animation": "^1.8.0",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-masonry-jothsa": "^0.0.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-stylelint": "^4.2.0"
  },
  "type": "module",
  "dependencies": {
    "@svelteuidev/composables": "^0.10.0",
    "@svelteuidev/core": "^0.10.0",
    "@svelteuidev/motion": "^0.10.0",
    "modern-normalize": "^1.1.0",
    "pocketbase": "^0.12.0",
    "svelte-media-queries": "^1.5.4"
  }
}
